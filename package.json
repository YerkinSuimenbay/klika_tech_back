    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.16",
    "@types/morgan": "^1.9.4",
    "@types/node": "^16.11.10",
    "morgan": "^1.10.0",
    "ts-node": "10.7.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "4.5.2"
  },
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "config": "^3.3.9",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "envalid": "^7.3.1",
    "express": "^4.17.2",
